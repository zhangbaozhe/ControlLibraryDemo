timeHorizon 3.0

ilqr
{
    nlocp_algorithm ILQR
	integrator EulerCT
	useSensitivityIntegrator true
	discretization Forward_euler
    dt 0.01
    K_shot 1
    K_sim 10
    epsilon 1e-6
	max_iterations 10
    fixedHessianCorrection false
    recordSmallestEigenvalue false
    min_cost_improvement 1e-12
    maxDefectSum 1e-5
    meritFunctionRho 1.0
    nThreads 1
    nThreadsEigen 1
    locp_solver GNRICCATI_SOLVER
    debugPrint false
    printSummary false
    loggingPrefix ILQR
    
    line_search
	{
		type NONE
		adaptive false ;
		maxIterations 10 ;
		alpha_0 1.0 ;
		n_alpha 0.5 ;
		debugPrint false
	}
}


gnms
{
    nlocp_algorithm GNMS
	integrator EulerCT
	useSensitivityIntegrator true
	discretization Forward_euler
    dt 0.01
    K_shot 60
    K_sim 10
    epsilon 1e-6
	max_iterations 10
    fixedHessianCorrection false
    recordSmallestEigenvalue false
    min_cost_improvement 1e-12
    maxDefectSum 1e-5
    nThreads 1    
    nThreadsEigen 1
    locp_solver GNRICCATI_SOLVER
    debugPrint false
    printSummary false
    loggingPrefix ILQR_GNMS_5
    
    line_search
	{
		active false;
		adaptive false ;
		maxIterations 10 ;
		alpha_0 1.0 ;
		n_alpha 0.5 ;
		debugPrint false
	}
}